ewActiveTab);\n    }\n    if (!cart || cart.length === 0) {\n        return (\n            <div className=\"bg-white h-full text-black flex justify-center p-4\">\n                <h1>Your Cart is empty</h1>\n            </div>\n        )\n    }\n    return (\n        <div className=\"bg-white h-screen text-black mx-auto mt-2 border border-gray-200 p-4 md:w-2/3 rounded-lg shadow-md sm:p-6 lg:p-8\">\n            <Tabs\n                list={categories}\n                activeTab={activeTab}\n                onTabSwitch={onTabSwitch}\n                />\n            <div className={`tabs ${activeTab !== 'Summary' ? 'hidden' : ''}`}>\n                    <ProductsSummary />\n                    <div className=\"flex justify-end p-2\">\n                    <Button variant=\"dark\" className=\"flex items-center\" onClick={()=>onTabSwitch('Delivery')}><span className=\"mr-1\">Next</span></Button>\n                    </div>\n            </div>\n            <div className={`tabs ${activeTab !== 'Delivery' ? 'hidden' : ''}`}>\n                <AddressForm onTabSwitch={onTabSwitch}></AddressForm>\n            </div>\n            <div className={`tabs ${activeTab !== 'Payment' ? 'hidden' : ''}`}>\n                <StripeWrapper></StripeWrapper>\n            </div>\n        </div>\n    )\n}\n\nexport default Cart;","/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/pages/SignUp/index.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/Banner.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/elements/Button.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/AboutUs.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/Products.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/ProductCard.jsx",["160"],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/AddToCart.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/stores/cart/cartSlice.js",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/stores/rootStore.js",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/stores/rootReducer.js",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/pages/Menu/index.jsx",["161"],[],"import { useEffect, useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { fetchProducts, getAllProducts } from \"../../stores/menu/productsSlice\";\nimport ProductDetailCard from \"../../components/ProductDetailCard\";\nimport { Tabs } from \"../../components/Tabs\";\nimport { addToCart } from \"../../stores/cart/cartSlice\";\n\nconst Menu = () => {\n    const dispatch = useDispatch();\n    const products = useSelector(getAllProducts);\n    const [activeTab, setActiveTab] = useState('');\n    const [activeTabIndex, setActiveTabIndex] = useState(0);\n    \n    // get products, which is empty, when mount\n    useEffect(() => {\n        dispatch(fetchProducts())\n    }, [])\n\n    const onAddProduct = (product) => {\n        dispatch(addToCart(product))\n    }\n\n    const onTabSwitch = (newActiveTab) => {\n        setActiveTab(newActiveTab);\n        let categories = products.products.map((product) => product.name.name);\n        let index = categories.findIndex(category => newActiveTab === category);\n        if (index > -1) {\n            setActiveTabIndex(index);\n        } else {\n            setActiveTabIndex(0);\n        }\n    }\n\n    return (\n        <div className=\"bg-white\">\n           {\n            products.status !== 'fulfilled' ?\n            <div>loading...</div> :\n            <div className=\"menu-wrapper\">\n                {\n                    products.products &&\n                    <Tabs\n                        list={products.products.map((product) => product.name.name)}\n                        activeTab={activeTab}\n                        onTabSwitch={onTabSwitch}\n                        />\n                }\n                <div className=\"flex flex-row mx-3\">\n                {\n                    products.products && products.products[activeTabIndex].products.map((product, index) => {\n                        return (\n                           <ProductDetailCard key={index} product={product} onAddProduct={onAddProduct}/>\n                        )\n                    })\n                }\n                </div>\n            </div>\n           }\n        </div>\n    )\n}\n\nexport default Menu;","/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/ProductDetailCard.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/Tabs.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/Tab.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/stores/menu/productsSlice.js",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/ProductsSummary.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/ProductSummaryCard.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/AddressForm.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/stores/userInfo/addressSlice.js",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/elements/Alert.jsx",[],[],"/Users/garysu/Github/Gym-Restaurant-Online-Ordering/src/components/PaymentForm.jsx",["162","163"],[],"import { CardElement, useElements, useStripe, Elements } from \"@stripe/react-stripe-js\";\nimport { loadStripe } from \"@stripe/stripe-js\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { clearCart, cartProducts } from \"../stores/cart/cartSlice\";\nimport { getAddress, clearAddress } from \"../stores/userInfo/addressSlice\";\nimport { useNavigate } from \"react-router-dom\";\nimport { useState, useEffect } from 'react';\nimport Button from \"./elements/Button\";\n\nconst stripePromise = loadStripe(process.env.REACT_APP_STRIPE_PUBLISHABLE_KEY);\n\nexport const StripeWrapper = () => {\n    return (\n        <Elements stripe={stripePromise}>\n            <PaymentForm />\n        </Elements>\n    )\n}\nconst PaymentForm = () => {\n    const [loading, setLoading] = useState(false);\n    const [error, setError] = useState(null);\n    const dispatch = useDispatch();\n    const cart = useSelector(cartProducts);\n    const address = useSelector(getAddress);\n    const navigate = useNavigate();\n    const elements = useElements();\n    const stripe = useStripe();\n\n    const handleSubmit = async (e) => {\n        e.preventDefault();\n\n        if (!stripe || !elements || !cart?.length || !address) {\n            return;\n        }\n\n        setLoading(true);\n        try {\n            const { error: backeEndError, clientSecret } = await fetch('https://gym-b.onrender.com/api/create-payment-intent', {\n                method: 'POST',\n                headers: {\n                    'Content-type': 'application/json'\n                },\n                body: JSON.stringify({\n                    paymentMethodType: 'card',\n                    orderItems: cart,\n                    userId: '',\n                    shippingAddress: address\n                })\n            }).then(r => r.json());\n\n            const { error: stripeError, paymentIntent } = await stripe.confirmCardPayment(\n                clientSecret, {\n                    payment_method: {\n                        card: elements.getElement(CardElement)\n                    }\n                }\n            )\n            if (backeEndError || stripeError) {\n                setError(backeEndError || stripeError)\n            } else if (paymentIntent.status === 'succeeded') {\n                dispatch(clearAddress());\n                dispatch(clearCart());\n                navigate('/paid');\n            }\n\n        } catch(err) {\n            console.log(err);\n        }\n\n        setLoading(false);\n    }\n\n    return (\n        <form className=\"md:-2/3 md:mx-auto px-2 pt-1\" id=\"payment-form\" onSubmit={handleSubmit}>\n            <label htmlFor=\"card-element\" className=\"pt-4 text-2xl md:text-center\">Please enter your card details</label>\n            <div className=\"my-4\">\n                <CardElement id=\"card-element\"/>\n            </div>\n            <div className=\"flex justify-center p-2\">\n                <Button type=\"submit\" disbled={loading}>\n                    {\n                        loading ?\n                        'Loading...' :\n                        'Pay Now'\n                    }\n                </Button>\n            </div>\n        </form>\n    )\n}",{"ruleId":"164","severity":1,"message":"165","line":1,"column":8,"nodeType":"166","messageId":"167","endLine":1,"endColumn":12},{"ruleId":"164","severity":1,"message":"168","line":1,"column":10,"nodeType":"166","messageId":"167","endLine":1,"endColumn":19},{"ruleId":"164","severity":1,"message":"169","line":11,"column":11,"nodeType":"166","messageId":"167","endLine":11,"endColumn":19},{"ruleId":"164","severity":1,"message":"170","line":9,"column":11,"nodeType":"166","messageId":"167","endLine":9,"endColumn":20},{"ruleId":"171","severity":1,"message":"172","line":17,"column":8,"nodeType":"173","endLine":17,"endColumn":10,"suggestions":"174"},{"ruleId":"164","severity":1,"message":"168","line":7,"column":20,"nodeType":"166","messageId":"167","endLine":7,"endColumn":29},{"ruleId":"164","severity":1,"message":"175","line":21,"column":12,"nodeType":"166","messageId":"167","endLine":21,"endColumn":17},"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'useEffect' is defined but never used.","'dispatch' is assigned a value but never used.","'loadImage' is assigned a value but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["176"],"'error' is a